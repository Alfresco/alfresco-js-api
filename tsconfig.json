{
    "compilerOptions":{
        "module": "commonjs",
        "sourceMap": true,
        "target": "es2015",
        "isolatedModules": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
    }
}
